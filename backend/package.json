{
  "name": "@clawban/backend",
  "version": "1.0.0",
  "description": "Clawban backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/backend/src/index.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.integration.config.js",
    "test:all": "npm test && npm run test:integration",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "migrate:assignee": "tsx src/migrate-assignee.ts",
    "audit:routes": "tsx scripts/audit-routes.ts",
    "precommit": "npm run lint && npm run typecheck && npm run audit:routes && npm run test:all"
  },
  "keywords": [
    "api",
    "express",
    "typescript"
  ],
  "author": "James Matlick",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "@supabase/supabase-js": "^2.93.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "nanoid": "^5.0.9",
    "prisma": "^7.3.0",
    "proper-lockfile": "^4.1.2",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/proper-lockfile": "^4.1.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.54.0"
  }
}
